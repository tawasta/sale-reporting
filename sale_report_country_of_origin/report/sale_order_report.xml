<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template
        id="report_saleorder_document"
        inherit_id="sale.report_saleorder_document"
    >
        <xpath expr="//th[@name='th_quantity']" position="after">
            <th name="th_country_of_origin" class="text-right">Country of <br
                />Origin</th>
        </xpath>
        <xpath expr="//td[@name='td_quantity']" position="after">
            <td name="td_country_of_origin" class="text-right">
                <span t-field="line.product_id.origin_country_id.code" />
            </td>
        </xpath>
    </template>
</odoo>
